{
    "name": "C++23 Dev Container",
    "build": {
      "dockerfile": "Dockerfile.cpp23_dev"
    },
    "settings": {
      "C_Cpp.default.compilerPath": "/usr/bin/g++",
      "C_Cpp.default.cppStandard": "c++23"
    },
    "extensions": [
      "ms-vscode.cpptools"
    ],
    "postCreateCommand": "g++ --version"
  }
  